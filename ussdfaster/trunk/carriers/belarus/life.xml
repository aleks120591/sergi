<?xml version="1.0" encoding="UTF-8"?>
<carrier
	title = "Life:)">
	<group title = "Info">
		<command
			title="Баланс"
			description="Баланс лицевого счета, количество бонусов, остаток Интернет-трафика, количестве SMS"
			template="*100#">
		</command>	
		<command
			title="Интернет"
			description="Остаток неиспользованного интернет-трафика и период действия"
			template="*105*4#">
		</command>
		<command
			title="Бонус"
			description="Количество бонусов и период действия"
			template="*105*9#">
		</command>
		<command
			title="Тарифный план"
			description="Ваш текущий тарифный план и номер телефона"
			template="*115#">
		</command>
	</group>
	<group title="Actions">
	  <command
			title="Перевод баланса"
			description="Перевести деньги на счет другого абонента"
			template="*120*1#">
		</command>
		<command
			title="Пополнить баланс"
			description="Пополнить лицевой счет с помощью скретч-карт"
			template="*100*{card}#">
			<field
				title="Код пополнения счета с карточки"
				tag="card"
				type="digits">
				<modifier
					type="complete"
					template="0000000000000000"
				/>
		  </field>
		</command>
		<command
			title="Любимые номера"
			description="Произвести операции с Любимыми номерами"
			template="*150#">
		</command>
		<command
			title="Пароль"
			description="Установить/изменить пароль"
			template="*120*9#">
		</command>
		<command
			title="Интернет 50 добавить"
			description="добавить тарифную услугу «Интернет 50»"
			template="*110*5*1*1*1#">
		</command>
		<command
			title="Интернет 50 удалить"
			description="удалить тарифную услугу «Интернет 50»"
			template="*110*5*1*1*2#">
		</command>
		<command
			title="Интернет 100 добавить"
			description="добавить тарифную услугу «Интернет 100»"
			template="*110*5*1*2*1#">
		</command>
		<command
			title="Интернет 100 удалить"
			description="удалить тарифную услугу «Интернет 100»"
			template="*110*5*1*2*2#">
		</command>
		<command
			title="Интернет 250 добавить"
			description="добавить тарифную услугу «Интернет 250»"
			template="*110*5*1*3*1#">
		</command>
		<command
			title="Интернет 250 удалить"
			description="удалить тарифную услугу «Интернет 250»"
			template="*110*5*1*3*2#">
		</command>
		<command
			title="Интернет 500 добавить"
			description="добавить тарифную услугу «Интернет 500»"
			template="*110*5*1*4*1#">
		</command>
		<command
			title="Интернет 500 удалить"
			description="удалить тарифную услугу «Интернет 500»"
			template="*110*5*1*4*2#">
		</command>
		<command
			title="Интернет на сутки 100 добавить"
			description="добавить тарифную услугу «Интернет на сутки 100»"
			template="*110*5*2*1*1#">
		</command>
		<command
			title="Интернет на сутки 100 удалить"
			description="удалить тарифную услугу «Интернет на сутки 100»"
			template="*110*5*2*1*2#">
		</command>
		<command
			title="Интернет на сутки 200 добавить"
			description="добавить тарифную услугу «Интернет на сутки 200»"
			template="*110*5*2*2*1#">
		</command>
		<command
			title="Интернет на сутки 200 удалить"
			description="удалить тарифную услугу «Интернет на сутки 200»"
			template="*110*5*2*2*2#">
		</command>		
	</group>
</carrier>
